@protocol = HTTP/1.1
@host = http://localhost
@port = 8080
@baseUrl = /v1/api/auth
@url = {{host}}:{{port}}{{baseUrl}}
@contentTypeJson = application/json

### signUp
POST {{url}}/signUp {{protocol}}
Content-Type: {{contentTypeJson}}

